        "# Create the line chart\n",
        "plt.figure(figsize=(10, 6))\n",
        "df_line.plot(kind='line', marker='o')\n",
        "\n",
        "# Add labels and title\n",
        "plt.xlabel('Year')\n",
        "plt.ylabel('Average Automobile Sales (in millions)')\n",
        "plt.title('Yearly Fluctuations of Automobile Sales')\n",
        "\n",
        "# Show grid for better readability\n",
        "plt.grid(True)\n",
        "\n",
        "# Display the plot\n",
        "plt.show()"
      ],
